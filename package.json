    "laravel-mix": "^4.1.2",
    "lodash": "^4.17.19",
    "sass-loader": "^7.3.1",
    "vue": "^2.6.11",
    "vue-parallaxy": "^1.1.1",
    "vue-router": "^3.0.3",
    "vue-slick": "^1.1.15"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "resolve-url-loader": "3.1.0",
    "style-resources-loader": "^1.2.1",
